class pa 
{
	public static void main(String[] args) 
	{
		int row = 5;
		int mid = (row/2)+1;
		int count = 0;
		for (int i = 1;i<=mid ;i++)
		{
			for (int j = 1;j<=mid ;j++ )
			{
				if (j==mid-i || j==mid+i  )
				{
					System.out.print("*");
				}
				else
					System.out.print(" ");
			}
			System.out.println();
		}
		
		}
	  
	  
		}
	
	

