class pattern222 
{
	public static void main(String[] args) 
	{
		int star = 25;
		int space = 1;
		int temp = star;
		for (int i =1 ;i<=star ;i++ )
		{
			for (int j = 1;j<=temp ;j++ )
			{
				System.out.print("*");
			}
			for (int j = 1;j<=space ;j++ )
			{
				if(i==1)
				{
					space = 1;
					break;
				}
				System.out.print(" ");
			}
			for (int j = 1;j<=temp ;j++ )
			{
				if(i==1&&j==temp)
					break;
			    System.out.print("*");
			}
			if(i!=1)
				space+=2;
			temp--;
			System.out.println();



		}


	}
}
